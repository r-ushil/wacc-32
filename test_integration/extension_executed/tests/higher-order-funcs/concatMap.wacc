import import-files/hof.wacc
begin
    int[] orig1 = [1, 2];
    int[] orig2 = [3, 4];
    int[][] orig = [orig1, orig2];

    int(int) triple = int(int x) is return x * 3 end;

    int[] res = call concatMap(triple, orig);

    int total = 0;
    for r : res do
        total = total + r
    done ;

    exit total # (1 * 3) + (2 * 3) + (3 * 3) + (4 * 3) = 30
end