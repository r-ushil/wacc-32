begin
    int[] map(int(int) f, int[] arr) is
        for a : arr do
            a = call f(a)
        done ;

        return arr
    end

    int[] filter(bool(int) f, int[] arr) is
        int count = 0;
        int[] tmp = new int[len arr];

        for a : arr do
            if f(a) then
                tmp[count] = a;
                count = count + 1
            else
                skip
            fi
        done ;

        int[] res = new int[count];

        for int i = 0; i < count; i = i + 1 do
            res[i] = tmp[i]
        done ;

        free tmp;

        return res

    end

    exit 0
end